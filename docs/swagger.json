    "swagger": "2.0",
    "info": {
        "description": "This is a sample server Petstore server.",
        "title": "github.com/1cool/golang-web Example API",
        "termsOfService": "https://github.com/1cool/golang-web",
        "contact": {
            "name": "1cool",
